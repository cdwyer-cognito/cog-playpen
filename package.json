  "description": "",
  "main": "build/index.js",
  "module": "./build/index.es.js",
  "scripts": {
    "build:devTODO": "npx webpack --config=webpack.development.config.js --mode=development",
    "build:prodTODO": "npx webpack --config=webpack.production.config.js",
    "test": "jest --verbose false",
    "testwatch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./storybookstaticfiles",
    "build:es2015": "cross-env NODE_ENV=production babel --config-file ./babel.config.js ./src --out-dir ./build --ignore \"./src/**/**/*.test.jsx,./src/**/**/*.stories.jsx\"",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel --config-file ./babel.config.js ./src/index.js --out-file ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build --ignore \"./src/**/**/*.test.jsx,./src/**/**/*.stories.jsx\"",
    "build:assets": "cross-env NODE_ENV=production babel --config-file ./babel.config.js ./src/assets --out-dir ./build/assets --copy-files",
    "build": "npm run build:es2015 && npm run build:es2015modules && npm run build:es && npm run build:assets"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest/globals.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdwyer-cognito/cog-playpen.git"
